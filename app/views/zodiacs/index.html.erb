<h1>List of Zodiacs</h1>

<table class="table table-striped table-hover">
  <tr>
    <th>Sign</th>
    <th>Creature</th>
    <th>Action</th>
  </tr>

  <% @list_of_zodiacs.each do |zodiac| %>
    <tr>
      <th><%= zodiac.sign %></th>
      <th><%= zodiac.creature %></th>
      <td><a href="http://localhost:3000/delete_zodiac/<%= zodiac.id %>">Delete</a></td>
    </tr>
  <% end %>
  </table>
